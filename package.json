{
    "name": "@cmmv/morgan",
    "version": "0.0.1",
    "description": "",
    "keywords": [
        "cmmv",
        "morgan",
        "logger"
    ],
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "types": "./dist/types/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/esm/index.js",
            "require": "./dist/cjs/index.js"
        }
    },
    "author": "Andre Ferreira",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/andrehrferreira/cmmv-morgan.git"
    },
    "bugs": {
        "url": "https://github.com/andrehrferreira/cmmv-morgan/issues"
    },
    "homepage": "https://cmmv.io",
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "node": ">= 18.18.0 || >= 20.0.0"
    },
    "lint-staged": {
        "**/*.ts": [
            "prettier --ignore-path ./.prettierignore --write"
        ]
    },
    "scripts": {
        "dev": "node -r @swc-node/register ./src/sample.ts",
        "build:cjs": "tsc --project tsconfig.cjs.json",
        "build:esm": "tsc --project tsconfig.esm.json",
        "build": "npm run build:cjs && npm run build:esm",
        "test": "",
        "prepare": "husky",
        "lint": "pnpm run lint:spec",
        "lint:fix": "pnpm run lint:spec -- --fix",
        "release": "node release.js",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
    },
    "devDependencies": {
        "@cmmv/http": "^0.8.1",
        "@cmmv/testing": "^0.8.1",
        "@commitlint/cli": "^19.6.0",
        "@commitlint/config-angular": "^19.6.0",
        "@commitlint/config-conventional": "^19.6.0",
        "@swc-node/core": "^1.13.3",
        "@swc-node/register": "^1.10.9",
        "@swc/core": "^1.10.1",
        "@swc/helpers": "^0.5.15",
        "@types/node": "^22.10.2",
        "@typescript-eslint/eslint-plugin": "^8.18.0",
        "@typescript-eslint/parser": "^8.18.0",
        "chalk": "^5.3.0",
        "conventional-changelog": "^6.0.0",
        "conventional-changelog-cli": "^5.0.0",
        "enquirer": "^2.4.1",
        "esbuild": "^0.24.0",
        "esbuild-register": "^3.6.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.31.0",
        "execa": "^9.5.2",
        "husky": "^9.1.7",
        "lint-staged": "^15.2.11",
        "prettier": "^3.4.2",
        "semver": "^7.6.3",
        "tslib": "^2.8.1",
        "typescript": "^5.7.2",
        "vitest": "^2.1.8"
    },
    "dependencies": {
        "@cmmv/core": "^0.8.1",
        "@cmmv/middleware": "^0.4.8",
        "@cmmv/server": "^0.7.2",
        "morgan": "^1.10.0"
    }
}